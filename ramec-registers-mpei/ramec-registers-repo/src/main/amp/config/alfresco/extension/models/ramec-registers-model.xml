             <mandatory>false</mandatory>
             <many>false</many>
           </target>
         </association>
         <association name="ramecedm:registable_addressee">
           <title>Адресат</title>
           <source>
             <mandatory>false</mandatory>
             <many>true</many>
           </source>
           <target>
             <class>cm:person</class>
             <mandatory>false</mandatory>
             <many>false</many>
           </target>
         </association>
         <association name="ramecedm:registable_informee">
           <title>Уведомить</title>
           <source>
             <mandatory>false</mandatory>
             <many>true</many>
           </source>
           <target>
             <class>cm:person</class>
             <mandatory>false</mandatory>
             <many>true</many>
           </target>
         </association>
       </associations>
     </aspect>
     <aspect name="ramecedm:addressable">
       <associations>
         <association name="ramecedm:addressees">
           <title>Кому</title>
           <source>
             <mandatory>false</mandatory>
             <many>true</many>
           </source>
           <target>
             <class>cm:person</class>
             <mandatory>false</mandatory>
             <many>true</many>
           </target>
         </association>
       </associations>
     </aspect>
     <aspect name="ramecedm:groupsaddressable">
       <associations>
         <association name="ramecedm:groupsaddressees">
           <title>Кому (группы)</title>
           <source>
             <mandatory>false</mandatory>
